# Environment variables for MCM Q3 project

# Data paths
DATA_DIR=data/
OUTPUT_DIR=outputs/
LOG_DIR=logs/

# Performance settings
MAX_WORKERS=4
MEMORY_LIMIT_GB=8
CHUNK_SIZE=2000

# Random seed for reproducibility
RANDOM_SEED=42

# Logging level
LOG_LEVEL=INFO

# Cache settings
CACHE_DIR=.cache
ENABLE_CACHE=true

# Model settings
DEFAULT_CONFIG=configs/q3_default.yaml

# Development settings
DEBUG=false
PROFILE=false